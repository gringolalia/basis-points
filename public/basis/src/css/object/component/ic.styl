selector = '[class^="' + _prefix + 'ic-"], [class*=" ' + _prefix + 'ic-"]';
{selector} {
  _ic();
}

/*
---
name: Icon (Angle Right)
category:
  - object/component
  - object/component/icon-font
---

```html
<span class="_ic-angle-right" aria-hidden="true"></span>
```
*/

.{_prefix}ic-angle-right::before {
  content: "\e900";
}

